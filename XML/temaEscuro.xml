<?xml version="1.0" encoding="utf-8"?>
<Config>
	<!-- Configurações Efeitos Degrade -->
	<GradientStartColor>FF000000</GradientStartColor>
	<GradientEndColor>FF3A3A3D</GradientEndColor>

	<!-- Configurações dos Formulários -->
	<FormBackgroundColor>FF1E1E1E</FormBackgroundColor>
	<FormFontFamily>Segoe UI</FormFontFamily>
	<FormFontSize>12</FormFontSize>
	<FormFontStyle>Bold</FormFontStyle>	

	<!-- Configurações do MenuStrip -->
	<MenuStripBackgroundColor>FF1E1E1E</MenuStripBackgroundColor>
	<GradientMenuStartColor>FF000000</GradientMenuStartColor>
	<GradientMenuEndColor>FF3A3A3D</GradientMenuEndColor>
	<MenuStripFontColor>FFFFFFFF</MenuStripFontColor>
	<MenuStripFontFamily>Segoe UI</MenuStripFontFamily>
	<MenuStripFontSize>12</MenuStripFontSize>
	<MenuStripFontStyle>Bold</MenuStripFontStyle>

	<!-- Configurações dos TextBox -->
	<TextBoxBackgroundColor>FF2D2D30</TextBoxBackgroundColor>
	<TextBoxFontColor>FFFFFFFF</TextBoxFontColor>
	<TextBoxBorderStyle>FixedSingle</TextBoxBorderStyle>
	<TextBoxFontFamily>Segoe UI</TextBoxFontFamily>
	<TextBoxFontSize>12</TextBoxFontSize>
	<TextBoxFontStyle>Regular</TextBoxFontStyle>

	<!-- Configurações do MaskedTextBox -->
	<MaskedTextBoxBackgroundColor>FF2D2D30</MaskedTextBoxBackgroundColor>
	<MaskedTextBoxFontColor>FFFFFFFF</MaskedTextBoxFontColor>
	<MaskedTextBoxBorderStyle>FixedSingle</MaskedTextBoxBorderStyle>
	<MaskedTextBoxFontFamily>Segoe UI</MaskedTextBoxFontFamily>
	<MaskedTextBoxFontSize>12</MaskedTextBoxFontSize>
	<MaskedTextBoxFontStyle>Regular</MaskedTextBoxFontStyle>

	<!-- Configurações dos Botões -->
	<ButtonBackgroundColor>FF3A3A3D</ButtonBackgroundColor>
	<ButtonFontColor>FFFFFFFF</ButtonFontColor>
	<ButtonAutoSize>false</ButtonAutoSize>
	<ButtonAppearance>
		<BorderColor>FF505050</BorderColor>
		<MouseDownBackColor>FF1E90FF</MouseDownBackColor>
		<MouseOverBackColor>FF4682B4</MouseOverBackColor>
	</ButtonAppearance>
	<ButtonFontFamily>Segoe UI</ButtonFontFamily>
	<ButtonFontSize>12</ButtonFontSize>
	<ButtonFontStyle>Bold</ButtonFontStyle>

	<!-- Configurações das Label -->
	<LabelBackgroundColor>FF2D2D30</LabelBackgroundColor>
	<LabelFontColor>FFFFFFFF</LabelFontColor>
	<LabelAutoSize>true</LabelAutoSize>
	<LabelFontFamily>Segoe UI</LabelFontFamily>
	<LabelFontSize>12</LabelFontSize>
	<LabelFontStyle>Bold</LabelFontStyle>

	<!-- Configurações do LinkLabel -->
	<LinkLabelActiveLinkColor>FF1E90FF</LinkLabelActiveLinkColor>
	<LinkLabelLinkColor>FF87CEFA</LinkLabelLinkColor>
	<LinkLabelVisitedLinkColor>FFB0C4DE</LinkLabelVisitedLinkColor>
	<LinkLabelFontFamily>Segoe UI</LinkLabelFontFamily>
	<LinkLabelFontSize>12</LinkLabelFontSize>
	<LinkLabelFontStyle>Underline</LinkLabelFontStyle>

	<!-- Configurações dos Panel -->
	<PanelBackgroundColor>FF2D2D30</PanelBackgroundColor>
	<PanelBorderStyle>Fixed3D</PanelBorderStyle>
	<PanelFontFamily>Segoe UI</PanelFontFamily>
	<PanelFontSize>12</PanelFontSize>
	<PanelFontStyle>Bold</PanelFontStyle>

	<!-- Configurações do ListView -->
	<ListViewBackColor>FF2D2D30</ListViewBackColor>
	<ListViewForeColor>FFFFFFFF</ListViewForeColor>
	<ListViewFullRowSelect>true</ListViewFullRowSelect>
	<ListViewGridLines>true</ListViewGridLines>
	<ListViewHideSelection>false</ListViewHideSelection>
	<ListViewFontFamily>Segoe UI</ListViewFontFamily>
	<ListViewFontSize>12</ListViewFontSize>
	<ListViewFontStyle>Bold</ListViewFontStyle>

	<!-- Configurações do ComboBox -->
	<ComboBoxBackColor>FF2D2D30</ComboBoxBackColor>
	<ComboBoxForeColor>FFFFFFFF</ComboBoxForeColor>
	<ComboBoxDropDownStyle>DropDownList</ComboBoxDropDownStyle>
	<ComboBoxFontFamily>Segoe UI</ComboBoxFontFamily>
	<ComboBoxFontSize>12</ComboBoxFontSize>
	<ComboBoxFontStyle>Regular</ComboBoxFontStyle>

	<!-- ToolStripMenuItem -->
	<ToolStripMenuItemBackColor>FF2D2D30</ToolStripMenuItemBackColor>
	<ToolStripMenuItemForeColor>FFFFFFFF</ToolStripMenuItemForeColor>
	<ToolStripMenuItemFontFamily>Segoe UI</ToolStripMenuItemFontFamily>
	<ToolStripMenuItemFontSize>12</ToolStripMenuItemFontSize>
	<ToolStripMenuItemFontStyle>Regular</ToolStripMenuItemFontStyle>

	<!-- Configurações do TabControl -->
	<TabControlFontFamily>Segoe UI</TabControlFontFamily>
	<TabControlFontSize>12</TabControlFontSize>
	<TabControlFontStyle>Regular</TabControlFontStyle>

	<!-- Configurações do TabPage -->
	<TabPageBackColor>FF2D2D30</TabPageBackColor>
	<TabPageForeColor>FFFFFFFF</TabPageForeColor>
	<TabPageBorderStyle>Fixed3D</TabPageBorderStyle>
	<TabPageFontFamily>Segoe UI</TabPageFontFamily>
	<TabPageFontSize>12</TabPageFontSize>
	<TabPageFontStyle>Bold</TabPageFontStyle>

	<!-- Configurações do ProgressBar -->
	<ProgressBarBackColor>FF2D2D30</ProgressBarBackColor>
	<ProgressBarForeColor>FF1E90FF</ProgressBarForeColor>
    <ProgressBarStyle>Continuous</ProgressBarStyle>
</Config>